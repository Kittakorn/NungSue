{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map"
      ],
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "library": "font-awesome@6.1.1",
      "destination": "wwwroot/lib/font-awesome",
      "files": [  "webfonts/*", "js/*" ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "library": "jquery.mask@1.14.16",
      "files": [ "jquery.mask.min.js" ],
      "destination": "wwwroot/lib/jquery-mask"
    },
    {
      "library": "bootstrap-maxlength@1.10.0",
      "files": [ "bootstrap-maxlength.min.js" ],
      "destination": "wwwroot/lib/bootstrap-maxlength"
    },
    {
      "library": "bootstrap-select@1.13.18",
      "files": [
        "css/bootstrap-select.min.css",
        "js/bootstrap-select.min.js",
        "js/bootstrap-select.min.js.map",
        "js/i18n/defaults-th_TH.min.js"
      ],
      "destination": "wwwroot/lib/bootstrap-select"
    },
    {
      "library": "iCheck@1.0.3",
      "files": [
        "skins/flat/blue.min.css",
        "skins/flat/blue.png",
        "skins/flat/blue@2x.png",
        "icheck.js"
      ],
      "destination": "wwwroot/lib/icheck"
    }
  ]
}